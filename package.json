{
  "name": "bilibili-fans",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "mock": "vite --mode mock",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest run",
    "coverage": "vitest run --coverage",
    "format": "npm run prettier && npm run lint",
    "prettier": "prettier --config ./.prettierrc --write src/**/*.{js,ts,jsx,tsx,vue,html,json}",
    "lint": "eslint --fix"
  },
  "dependencies": {
    "mockjs": "^1.1.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@iconify-json/uil": "^1.1.2",
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "naive-ui": "^2.33.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "typescript": "^4.8.2",
    "unocss": "^0.45.13",
    "vfonts": "^0.0.3",
    "vite": "^3.0.9",
    "vite-plugin-mock": "^2.9.6",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.39.5"
  }
}